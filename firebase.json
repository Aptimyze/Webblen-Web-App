{
  "hosting": {
    "public": "build/web",
    "headers": [ {
      "source" : "index.html",
      "headers" : [ {
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ]
    }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [
      {
        "source": "/shared_link/**",
        "dynamicLinks": true
      },
      {
        "source": "!/shared_link/**",
        "destination": "/index.html"
      }
    ]
  }
}
